<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bdqn.his.masterdata.mapper.item.CheckItemMapper">
    <select id="getItems" resultType="CheckItem">
        select *,type.type_value typeName from check_item,type
        where type.group=3 and type.type_id=check_item.type
    </select>
</mapper>    